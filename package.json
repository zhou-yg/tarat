  },
  "homepage": "https://github.com/zhou-yg/tarat#readme",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^18.6.2",
    "eslint": "^8.16.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@jsdevtools/version-bump-prompt": "6.1.0",
    "chalk": "3.x.x",
    "chokidar": "^3.5.3",
