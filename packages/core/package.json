{
  "name": "@tarot-run/core",
  "version": "0.0.5",
  "description": "useHook serverless framework",
  "main": "./dist/index.server.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "watch-unit": "jest --watch -- __test__/core/computed.test.ts",
    "test-unit": "jest",
    "test-ci": "TEST=CI jest",
    "format": "prettier --write --parser typescript \"*/*.ts?(x)\"",
    "build": "rm -rf dist/ && npm run format && rollup --config rollup.config.mjs",
    "build:watch": "npm run format && rollup --config rollup.config.mjs --watch",
    "release": "npm run build && bump --commit \"release: @tarot-run/core v%s\" --push && npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhou-yg/tarot.git"
  },
  "author": "zhouyg",
  "bugs": {
    "url": "https://github.com/zhou-yg/tarot/issues"
  },
  "homepage": "https://github.com/zhou-yg/tarot#readme",
  "dependencies": {
    "@vue/reactivity": "^3.2.36",
    "immer": "^9.0.14",
    "rollup-plugin-dts": "^4.2.2",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@jsdevtools/version-bump-prompt": "6.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "execa": "6.x.x",
    "rollup": "^2.74.1",
    "rollup-plugin-typescript2": "^0.31.2"
  }
}
