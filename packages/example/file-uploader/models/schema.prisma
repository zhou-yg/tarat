datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider        = "prisma-client-js"
  output          = "./customPrismaClient/client"
  previewFeatures = ["fullTextSearch", "fullTextIndex"]
}

model StorageItem {
  id          Int      @id @default(autoincrement())
  createdAt   DateTime @default(now())
  modifiedAt  DateTime @default(now())

  name String
  link String
}
